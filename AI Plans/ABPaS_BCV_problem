(define (problem STOP_Action) (:domain ABPaS_domain)
    (:objects 
        l_val r_val u_val s_val bcv st_val t_val buzz_val exh_Val pir_val d_val b_val
    )
    (:init
    (Is_Start s_val)
(Is_No_Ultrasonic u_val)
(Is_Both_IR b_val)
 )
    (:goal (or 
    (Obs_Detected bcv)
    (Move_Forward bcv)
    (Move_Stop bcv)
    (Move_Left bcv) 
    (Move_Right bcv) 
    )

    ))